{
    "name": "cassarco/league-commonmark-wikilinks",
    "description": "An extension that enables wikilinks parsing in league/commonmark.",
    "keywords": [
        "cassarco",
        "wikilinks",
        "markdown",
        "league-commonmark-wikilinks"
    ],
    "homepage": "https://github.com/cassarco/league-commonmark-wikilinks",
    "license": "MIT",
    "authors": [
        {
            "name": "Cassar & Co",
            "email": "mail@cassar.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "league/commonmark": "^2.4.2"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "laravel/pint": "^v1.13.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Cassarco\\LeagueCommonmarkWikilinks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cassarco\\LeagueCommonmarkWikilinks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
